        <th scope="col">Ticker</th>
        <th scope="col">Name</th>
        <th scope="col">Price</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
    <% @user_stocks.each do |user_stock| %>
      <tr>
        <th><%= user_stock.ticker %></th>
